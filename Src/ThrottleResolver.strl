module ThrottleResolver:

input UseAuto;
input AutoThrottleCmd : float;  % Value from PID
input Accel : float;

output ThrottleCmd : float;
output UsingAutoInThrottleResolver;

loop
    present UseAuto then
        emit UsingAutoInThrottleResolver;
        emit ThrottleCmd(?AutoThrottleCmd);
    else
        emit ThrottleCmd(?Accel);
    end present;
each tick;

end module
