module SetCruiseSpeed:

input SetSpeed;
input Speed : float;
output CruiseSpeed : float;
constant SpeedMin = 30.0f : float;
constant SpeedMax = 150.0f : float; 

loop
	present (SetSpeed) then
		if ?Speed > SpeedMin and ?Speed < SpeedMax then
			emit CruiseSpeed(?Speed);
		end if;
	end present;
each tick;

end module
