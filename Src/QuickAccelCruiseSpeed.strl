module QuickAccelCruiseSpeed:

input QuickAccel;
input Speed : float;
output CruiseSpeed : float;
constant SpeedInc = 2.5f : float; 
constant SpeedMax = 150.0f : float;

loop
	present (QuickAccel)
		if (?Speed + ?SpeedInc) < SpeedMax then
			emit CruiseSpeed(?Speed + ?SpeedInc);
		end if;
	end present;
each tick;

end module
