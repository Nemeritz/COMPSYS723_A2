module QuickDecelCruiseSpeed:

input QuickDecel;
input Speed : float;
output CruiseSpeed : float;
constant SpeedInc = 2.5f : float; 
constant SpeedMin = 30.0f : float;

loop
	present (QuickDecel)
		if (?Speed - ?SpeedInc) > SpeedMin then
			emit CruiseSpeed(?Speed - ?SpeedInc);
		end if;
	end present;
each tick;

end module
